<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="cordova-plugin-sunmistore"
        version="1.0.2">

  <name>SunmiStore</name>

  <engines>
    <engine name="cordova" version=">=3.4.0"/>
  </engines>

  <asset src="www/sunmistore.js" target="js/sunmistore.js"/>

  <js-module src="www/sunmistore.js" name="sunmistore">
    <clobbers target="sunmistore" />
  </js-module>

  <platform name="android">
	<config-file target="res/xml/config.xml" parent="/*">
      <feature name="SunmiStore">
        <param name="android-package" value="com.openmove.sunmistore.SunmiStore"/>
      </feature>
    </config-file>
	 <source-file src="src/android/SunmiStore.java" target-dir="src/com/openmove/sunmistore/"/>
  </platform>


</plugin>
